class LineBundle {
    LineBundle() {
    }
}

class Line {
    float x;
    float v;
    boolean leaf;
    Line left;
    Line right;

    Line() {
    }

    void move(float dir) {
        if (leaf) {
            x += v;
        }
        else {
            left.move();
            right.move()
        }
    }

    void adjustSpeed() {
    }
  
    boolean leftmost() {
        if (leaf) {
            return x <= 0;
        }
        else {
            return left.leftmost();
        }
    }

    boolean rightmost() {
        if (leaf) {
            return x >= width;
        }
        else {
            return right.rightmost();
        }
    }
  
    void display() {
    }
}

void setup() {
}

void draw() {
}
