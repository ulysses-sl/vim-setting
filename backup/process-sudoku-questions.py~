filename = None
writefile = None


for line in open("p096_sudoku.txt", "r"):
    if line[0] not in "0123456789":
        if filename:
            writefile.close()
        filename = "sudoku-questions/" + line.split()[1] + "txt"
        writefile = open(filename, "wt")
    else:
        writefile.write(line.replace('', ' '))

writefile.close()
