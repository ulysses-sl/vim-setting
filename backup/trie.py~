class Node:
    def __init__(letter):
        self.letter = letter
        self.next = {}
        self.final = False

    def add(word):
        if not word:
            self.final = True
        else:
            letter = word[0]
            rest = word[1:]
            if letter not in self.next.keys():
                self.next[letter] = Node(letter)
            self.next[letter].add(rest)

    def exists(word):
        if not word:
            return self.final
        else:
            letter = word[0]
            rest = word[1:]
            if letter not in self.next.keys():
                return False
            self.next[letter].exists(rest)


class Trie:
    def __init__():
        self.root = Node(0)

    def add(word):
        self.root.add(word)

    def exists(word):
        self.root.exists(word)
